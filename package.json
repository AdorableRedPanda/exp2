	"devDependencies": {
		"typescript": "5.9.2",
		"@types/node": "24.3.3",
		"@types/react": "19.1.13",
		"@types/react-dom": "19.1.9",
		"@tailwindcss/postcss": "4.1.13",
		"tailwindcss": "4.1.13",
		"@biomejs/biome": "2.2.4",
		"@typescript-eslint/eslint-plugin": "8.43.0",
		"@typescript-eslint/parser": "8.43.0",
		"eslint": "9.35.0",
		"eslint-plugin-perfectionist": "4.15.0",
		"lefthook": "1.13.0"
